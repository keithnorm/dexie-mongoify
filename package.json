    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-matchers": "^3.0.1",
    "karma-mocha-reporter": "^2.2.0",
    "webpack": "^1.15.0"
