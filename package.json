{
  "name": "dexie-mongoify",
  "version": "1.2.3",
  "description": "Dexie.js plugin with MongoDB-like API",
  "author": {
    "name": "YurySolovyov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YuriSolovyov/Dexie-mongoify"
  },
  "scripts": {
    "lint": "eslint src",
