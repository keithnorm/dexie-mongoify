    "test:debug": "node_modules/.bin/karma start karma-test.conf.js --browsers Chrome",
    "build": "webpack -p",
    "build:watch": "webpack --watch",
    "bench": "node_modules/.bin/karma start karma-bench.conf.js --single-run --browsers Chrome",
